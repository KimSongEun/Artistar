<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ArtistInsert">
	<insert id="artistInsertRequest" parameterType="hashmap">
		INSERT INTO ARTIST_INSERT 
		VALUES(ARTIST_INSERT_SEQ.NEXTVAL, #{userId}, #{artistInsert.artist_name}, #{artistInsert.artist_intro}, 
		       #{artistInsert.artist_country}, #{artistInsert.artist_gender}, #{artistInsert.artist_birth}, #{artistInsert.artist_dead}, 
		       #{artistInsert.artist_img}, #{artistInsert.artist_detail}, 0)
	</insert>
</mapper>
