<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ArtistDelete">
	<insert id="artistDeleteRequest" parameterType="hashmap">
		INSERT INTO ARTIST_DELETE 
		VALUES(ARTIST_DELETE_SEQ.NEXTVAL, #{artistNum}, #{userId}, #{reason}, 0)
	</insert>	
</mapper>
